{
    "manifest_version": 2,
    "name": "Standups",
    "description": "Collaboration tool for daily stand-ups meetings.",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "index.html"
    }
}
